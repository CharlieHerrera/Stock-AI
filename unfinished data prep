        "def get_stock_data(ticker, start=\"2015-01-01\", end=\"2024-12-31\"):\n",
        "    data = yf.download(ticker, start=start, end=end)\n",
        "    return data\n",
        "\n",
        "# Example usage\n",
        "df = get_stock_data(\"AAPL\")\n",
        "df.head()\n"
      ],
